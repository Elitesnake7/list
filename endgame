//[Sockets] == "6" && [Linked] <= "5" && [Rarity] == "Magic" 		# [SellUnid] == "true" // Sell 6S Magic
//[Sockets] == "6" && [Linked] <= "5" && [Rarity] == "Rare" 		# [SellUnid] == "true" // Sell 6S Rare

